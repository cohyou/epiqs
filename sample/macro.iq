'> ^> -1 [
  |# macro |\ '% [otag p q] ^> -1 [
    |? @eq! [@otag "Add"]
       |: @plus! [@p @q]
          '> |? @eq! [@otag "Html"]
                |: @concat! ["<html>" @macro! [@q.q.p.o @q.q.p.p @q.q.p.q] "</html>"]
                   '> |? @eq! [@otag "Body"]
                         |: "<body></body>"
                            "none"
  ]

  |Add 1 2
  'Html ['Head "" 'Body ""]
]
